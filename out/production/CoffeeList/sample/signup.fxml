<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="559.0" prefWidth="394.0" style="-fx-background-color: #F5F5F6;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Signup">
   <children>
      <AnchorPane prefHeight="114.0" prefWidth="394.0" style="-fx-background-color: #546e7a;">
         <children>
            <Text fill="WHITE" layoutX="141.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Coffee List">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
      <JFXButton fx:id="signupButton" layoutX="153.0" layoutY="438.0" onAction="#signupButtonClick" style="-fx-background-color: #546e7a;" text="Sign Up" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="loginButton" layoutX="162.0" layoutY="498.0" onAction="#loginButtonClick" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </JFXButton>
      <JFXTextField fx:id="usernameInput" layoutX="96.0" layoutY="165.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="passwordInput" layoutX="96.0" layoutY="292.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </JFXPasswordField>
      <JFXTextField fx:id="emailInput" layoutX="96.0" layoutY="226.0" promptText="Email">
         <font>
            <Font size="18.0" />
         </font>
      </JFXTextField>
      <JFXPasswordField fx:id="confirmPasswordInput" layoutX="96.0" layoutY="359.0" promptText="Confirm Password">
         <font>
            <Font size="18.0" />
         </font>
      </JFXPasswordField>
   </children>
</AnchorPane>
